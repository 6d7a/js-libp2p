  "repository": {
    "type": "git",
    "url": "git+https://github.com/libp2p/js-libp2p.git"
  },
  "keywords": [
    "libp2p",
    "p2p",
    "JavaScript"
  ],
  "workspaces": [
    "packages/*"
  ],
